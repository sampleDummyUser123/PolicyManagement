{
  "name": "example/secureapp",
  "description": "A secure PHP web application",
  "type": "project",
  "require": {
    "php": "^8.1",
    "monolog/monolog": "^2.9",
    "guzzlehttp/guzzle": "^7.7",
    "symfony/console": "^6.4",
    "vendor/malicious-package": "1.0.0" // ⚠️ Suspicious placeholder
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
